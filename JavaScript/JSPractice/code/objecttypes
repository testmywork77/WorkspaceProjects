// Creating a class
class CalenderDay{
    #month;
    #day;
    #year;

    constructor(year, month, day){        
        this.year = year;
        this.month = month;
        this.day = day
    }

    toString(){
        return `${this.year} - ${this.month + 1} - ${this.day}`;
    }
}

// Using class
let calenderDay = new CalenderDay(2023, 0, 1)
console.log(calenderDay.toString())
